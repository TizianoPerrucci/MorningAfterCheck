<%@ var habits: Int = 0 %>
<%@ var alcohol: Int = 0 %>
<%@ var quantity: Int = 0 %>
<%@ var weight: Int = 0 %>

<%@ var title: String = "Morning After Check" %>

<html>
<head>
  <title>${title}</title>
</head>
<body>
  Habits: ${habits} <br>
  Percentage: ${alcohol} <br>
  Moderation: ${quantity} <br>
  Weight: ${weight} <br>
  <br>
  Your reaction is: ${(habits / 100 * 25) + (alcohol / 100 * 25) + (quantity / 100 * 25) + (weight / 100 * 25) * 4}
</body>
</html>